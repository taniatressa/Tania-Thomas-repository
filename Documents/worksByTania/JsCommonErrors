function make_obj(firstName,lastName){
this.firstName = firstName;
this.lastName = lastName;
}

var obj1 = make_obj("tania","tressa"); //make_obj doesnt return anything
//console.log(obj1.firstName); //typeError since there is now firstname refer to obj1.bcz we dint invoke with new keyword
