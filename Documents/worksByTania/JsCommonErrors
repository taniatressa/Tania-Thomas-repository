//-----------------------------------------------------------------------
function MyConstructor() {}
MyConstructor.prototype = {};
var myobject = new MyConstructor();

console.log(myobject instanceof MyConstructor); //true
console.log(myobject instanceof Object); //true
console.log(myobject.constructor == MyConstructor) //false

//----------------------------------------------------------------

function MyConstructor() {}
var myobject = new MyConstructor();
MyConstructor.prototype = {};

console.log(myobject instanceof MyConstructor);//false
console.log(myobject instanceof Object);//true
console.log(myobject.constructor == MyConstructor) //true

//---------------------------------------------------------------

function MyConstructor() {}
var myobject = new MyConstructor();

console.log(myobject instanceof MyConstructor);//true
console.log(myobject instanceof Object); //true
console.log(myobject.constructor == MyConstructor) //false

//-------------------------------------------------------------


